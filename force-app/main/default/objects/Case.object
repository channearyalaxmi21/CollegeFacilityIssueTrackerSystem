    <validationRules>
        <fullName>Prevent_Close_Without_Completion_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Status, &quot;Closed&quot;), ISBLANK(Repair_Completion_Date__c))</errorConditionFormula>
        <errorMessage>&quot;Enter Repair Completion Date before closing the Case.&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Category_Location</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
    ISPICKVAL(Category__c, &quot;&quot;), 
    ISBLANK(Location_Detail__c)
)</errorConditionFormula>
        <errorMessage>&quot;Please select Category and enter Location.&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Cost_For_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Requires_Approval__c, ISBLANK(Estimated_Cost__c))</errorConditionFormula>
        <errorMessage>&quot;Enter Estimated Cost before requesting approval.&quot;</errorMessage>
    </validationRules>
